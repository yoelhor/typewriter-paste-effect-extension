{
    "name": "Typewriter effect",
    "version": "0.0.0.11",
    "manifest_version": 3,
    "description": "An extension to paste with a typewriter effect.",
    "permissions": [
        "clipboardRead",
        "activeTab",
        "scripting"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {},
    "commands": {
        "_execute_action": {
            "suggested_key": {
                "default": "Ctrl+Shift+L",
                "mac": "Ctrl+Shift+L"
            }
        }
    }
}